{
  "name": "shades-figma",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "run-s dev",
    "dev": "run-s watch",
    "dev:ui-only": "vite -c ./vite.config.ui.ts",
    "watch": "run-p watch:*",
    "watch:ui": "npm run build:ui -- --watch --mode=development",
    "watch:plugin": "npm run build:plugin -- --watch --mode=development",
    "clean": "rimraf dist/*",
    "types": "run-s types:*",
    "types:src": "tsc",
    "types:node": "tsc -P tsconfig.node.json",
    "build": "run-s build:*",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.18",
    "@tanstack/react-query": "^5.51.11",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@types/chroma-js": "^3.1.1",
    "@types/color-namer": "^1.3.3",
    "axios": "^1.7.2",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "color-namer": "^1.4.0",
    "figx": "^0.1.3",
    "immer": "^10.1.1",
    "monorepo-networker": "^1.5.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.10.3",
    "react-tiny-popover": "^8.1.6",
    "superjson": "^2.2.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.108.0",
    "@types/node": "^18.15.11",
    "@types/postcss-url": "^10.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-url": "^10.1.3",
    "rimraf": "^5.0.5",
    "sass": "^1.60.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-generate-file": "^0.1.1",
    "vite-plugin-react-rich-svg": "^1.0.0",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
